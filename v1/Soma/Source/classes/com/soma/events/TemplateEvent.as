package com.soma.events {		import flash.events.Event;		import com.soma.view.Page;		import com.soma.control.CairngormEvent;		/**	 * <b>Author:</b> Romuald Quantin - <a href="http://www.soundstep.com/" target="_blank">www.soundstep.com</a><br />	 * <b>Class version:</b> BETA 1.0<br />	 * <b>Actionscript version:</b> 3.0<br />	 * <b>Copyright:</b> 	 * <br />	 * <b>Date:</b> 05-2008<br />	 * <b>Usage:</b>	 * @example	 * <listing version="3.0"></listing>	 */	 	public class TemplateEvent extends CairngormEvent {				//------------------------------------		// private properties		//------------------------------------										//------------------------------------		// public properties		//------------------------------------				public static const TEMPLATE_DISPLAYED:String = "templateDisplayed";		public static const TEMPLATE_REMOVED:String = "templateRemoved";				public var page:Page;		
		//------------------------------------		// constructor		//------------------------------------				public function TemplateEvent(type:String, page:Page, bubbles:Boolean = false, cancelable:Boolean = false) {			super(type, bubbles, cancelable);			this.page = page;		}				//		// PRIVATE		//________________________________________________________________________________________________								// PUBLIC		//________________________________________________________________________________________________				public override function clone():Event {			return new TemplateEvent(type, page, bubbles, cancelable);		}				public override function toString():String {			return formatToString("TemplateEvent", "page", "type", "bubbles", "cancelable", "eventPhase");		}			}}